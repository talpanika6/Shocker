
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/login_bg"
    android:id="@+id/mainRegister"
    tools:ignore="MergeRootFrame" >


    <RelativeLayout
        android:id="@+id/RegisterContainer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" >


    <LinearLayout
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical"
        >

        <android.support.v7.widget.CardView
            android:id="@+id/sighupView"
            android:layout_gravity="center"
            android:layout_width="250dp"
            android:layout_height="200dp"
            android:layout_marginStart="15dp"
            android:layout_marginTop="130dp"
            android:layout_marginEnd="15dp"
            android:elevation="8dp"
            card_view:cardBackgroundColor="#D7CCC8"
            card_view:cardCornerRadius="2dp">


    <EditText
        android:id="@+id/user_name_edit_text"
        android:layout_width="wrap_content"
        android:textStyle="normal|bold|italic"
        android:fontFamily="sans-serif-medium"
        android:layout_height="wrap_content"
        android:inputType="text"
        android:textColorHint="#fdfeef"
        android:hint="@string/prompt_user_name"
        android:ems="10"
        android:layout_marginStart="20dp"
        android:layout_marginTop="30dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:clickable="false"
        android:phoneNumber="false" />
    <EditText

        android:textStyle="normal|bold|italic"
        android:fontFamily="sans-serif-medium"
    android:id="@+id/password_edit_text"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:inputType="textPassword"
      android:password="true"
    android:hint="@string/prompt_password"
    android:textColorHint="#fdfeef"
    android:ems="10"
    android:layout_marginStart="20dp"
    android:layout_marginTop="80dp"
    android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
    android:clickable="false"
    android:singleLine="false" />

    <EditText
        android:textStyle="normal|bold|italic"
        android:fontFamily="sans-serif-medium"
        android:id="@+id/password_again_edit_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPassword"
        android:password="true"
        android:hint="@string/prompt_password_again"
        android:textColorHint="#fdfeef"
        android:ems="10"
        android:layout_marginStart="20dp"
        android:layout_marginTop="130dp"
        android:layout_alignParentStart="true"
        android:imeActionId="@+id/edittext_action_signup"
        android:imeOptions="actionUnspecified"
        android:singleLine="false" />
        </android.support.v7.widget.CardView>
    <Button
        android:id="@+id/sigh_up_btn"
        android:text="@string/sign_up_btn"
        android:textColor="#fdfeef"
        android:textAllCaps="false"
        android:textStyle="normal|italic"
        android:fontFamily="sans-serif-medium"
        android:background="@drawable/button_sighup"
        android:textSize="20sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="140dp"
        android:layout_marginTop="30dp"
        android:onClick="Login"
        android:clickable="true" >


        </Button>

        <TextView
            android:id="@+id/textview_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="150dp"
            android:layout_marginTop="100dp"
            android:layout_marginBottom="0dp"
            android:textStyle="normal|bold|italic"
            android:fontFamily="sans-serif-medium"
            android:text="@string/powered_by"
            android:textColor="@color/window_background"
            android:textAppearance="?android:attr/textAppearanceSmall" />

        <TextView
            android:id="@+id/textview_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="160dp"
            android:layout_marginTop="5dp"

            android:textColor="@color/window_background"
            android:textStyle="normal|bold|italic"
            android:fontFamily="sans-serif-medium"
            android:layout_marginBottom="15dp"
            android:text="@string/ramit"
            android:textAppearance="?android:attr/textAppearanceMedium" />


  </LinearLayout>
    </RelativeLayout>



</FrameLayout>